{
  "name": "diflexmo-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run diflexmo-admin:storybook",
    "build-storybook": "ng run diflexmo-admin:build-storybook",
    "lint": "ng lint",
    "prepare": "husky install",
    "openapi-gen": "ng-openapi-gen -i http://localhost:7002/swagger/v1/swagger.json -o src/app/core/services"
